<% if @snapshot.persisted? %>
  // redirect to snapshot page
  window.location.href = "<%= snapshot_url(@snapshot) %>";
<% else %>
  console.info("not persisted. validation errors presumably present.");

  <% @snapshot.errors.full_messages.each do |msg| %>
    console.error("<%= msg %>");
  <% end %>

  $('body').addClass('error').removeClass('uploading');
<% end %>
